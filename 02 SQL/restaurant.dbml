Table customers {
  id int [PK]
  firstname text [not null]
  lastname text [not null]
  address text
  email text
  phone text
}

Table menus_head {
  id int [PK]
  menu_name text [not null]
}

Table menus_detail{
  id int [PK]
  size text [not null]
  price real [not null]
  menu_id int [not null, ref: > menus_head.id]
}

Table payment_type {
  id int [PK]
  payment text [not null]
}

Table orders_head {
  id int [PK]
  order_date date [not null]
  customer_id int [not null, ref: > customers.id]
  payment_id int [not null, ref: - payment_type.id]
}


Table orders_detail {
  id int [PK]
  order_id int [not null, ref: > orders_head.id]
  menu_detail_id int [not null,ref: - menus_detail.id]
  quantity int [not null]
}